@echo off
IF "%1" EQU "dev" (
  IF %2 EQU "vue" (
    @IF EXIST "%~dp0\node.exe" (
      "%~dp0\node.exe"  "%~dp0\..\webpack-dev-server\bin\webpack-dev-server.js" --inline --progress --config %~dp1%~2\%~3\build\webpack.dev.conf.js
    ) ELSE (
      @SETLOCAL
      @SET PATHEXT=%PATHEXT:;.JS;=;%
      node  "%~dp0\..\webpack-dev-server\bin\webpack-dev-server.js" --inline --progress --config %~dp1%~2\%~3\build\webpack.dev.conf.js
    )
  ) ELSE IF %2 EQU "nuxt" (
    @IF EXIST "%~dp0\node.exe" (
      "%~dp0\node.exe"  "%~dp0\..\nuxt\bin\nuxt.js" %~dp1%~2\%~3
    ) ELSE (
      @SETLOCAL
      @SET PATHEXT=%PATHEXT:;.JS;=;%
      node  "%~dp0\..\nuxt\bin\nuxt.js" %~dp1%~2\%~3
    )
  ) ELSE IF %2 EQU "vue3" (
    SET VUE_CLI_CONTEXT=%~dp1%~2\%~3
    @IF EXIST "%~dp0\node.exe" (
      "%~dp0\node.exe"  "%~dp0\..\@vue\cli-service\bin\vue-cli-service.js" serve
    ) ELSE (
      @SETLOCAL
      @SET PATHEXT=%PATHEXT:;.JS;=;%
      node  "%~dp0\..\@vue\cli-service\bin\vue-cli-service.js" serve
    )
  )
) ELSE IF "%1" EQU "build" (
  IF %2 EQU "vue" (
    node %~2/%~3/build/build.js
  ) ELSE IF %2 EQU "nuxt" (
    @IF EXIST "%~dp0\node.exe" (
      "%~dp0\node.exe"  "%~dp0\..\nuxt\bin\nuxt" %1 %~dp1%~2\%~3
    ) ELSE (
      @SETLOCAL
      @SET PATHEXT=%PATHEXT:;.JS;=;%
      node  "%~dp0\..\nuxt\bin\nuxt" %1 %~dp1%~2\%~3
    )
  ) ELSE IF %2 EQU "vue3" (
     SET VUE_CLI_CONTEXT=%~dp1%~2\%~3
    @IF EXIST "%~dp0\node.exe" (
      "%~dp0\node.exe"  "%~dp0\..\@vue\cli-service\bin\vue-cli-service.js" %1
    ) ELSE (
      @SETLOCAL
      @SET PATHEXT=%PATHEXT:;.JS;=;%
      node  "%~dp0\..\@vue\cli-service\bin\vue-cli-service.js" %1
    )
  )
) ELSE IF "%1" EQU "start" (
  IF %2 EQU "nuxt" (
    @IF EXIST "%~dp0\node.exe" (
      "%~dp0\node.exe"  "%~dp0\..\nuxt\bin\nuxt" %1 %~dp1%~2\%~3
    ) ELSE (
      @SETLOCAL
      @SET PATHEXT=%PATHEXT:;.JS;=;%
      node  "%~dp0\..\nuxt\bin\nuxt" %1 %~dp1%~2\%~3
    )
  ) ELSE (
    echo %~2 is no such command
  )
) ELSE IF "%1" EQU "generate" (
  IF %2 EQU "nuxt" (
    @IF EXIST "%~dp0\node.exe" (
      "%~dp0\node.exe"  "%~dp0\..\nuxt\bin\nuxt" %1 %~dp1%~2\%~3
    ) ELSE (
      @SETLOCAL
      @SET PATHEXT=%PATHEXT:;.JS;=;%
      node  "%~dp0\..\nuxt\bin\nuxt" %1 %~dp1%~2\%~3
    )
  ) ELSE (
    echo %~2 is no such command
  )
) ELSE IF "%1" EQU "lint" (
  IF %2 EQU "vue3" (
    SET VUE_CLI_CONTEXT=%~dp1%~2\%~3
    @IF EXIST "%~dp0\node.exe" (
      "%~dp0\node.exe"  "%~dp0\..\@vue\cli-service\bin\vue-cli-service.js" %1
    ) ELSE (
      @SETLOCAL
      @SET PATHEXT=%PATHEXT:;.JS;=;%
      node  "%~dp0\..\@vue\cli-service\bin\vue-cli-service.js" %1
    )
  )
)