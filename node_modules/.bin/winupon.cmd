@echo off
IF %1 EQU "vue" (
  @IF EXIST "%~dp0\node.exe" (
    "%~dp0\node.exe"  "%~dp0\..\webpack-dev-server\bin\webpack-dev-server.js" --inline --progress --config %~dp1%~1\%~2\build\webpack.dev.conf.js
  ) ELSE (
    @SETLOCAL
    @SET PATHEXT=%PATHEXT:;.JS;=;%
    node  "%~dp0\..\webpack-dev-server\bin\webpack-dev-server.js" --inline --progress --config %~dp1%~1\%~2\build\webpack.dev.conf.js
  )
) ELSE IF %1 EQU "nuxt" (
  @IF EXIST "%~dp0\node.exe" (
    "%~dp0\node.exe"  "%~dp0\..\nuxt\bin\nuxt.js" %~dp1%~1\%~2
  ) ELSE (
    @SETLOCAL
    @SET PATHEXT=%PATHEXT:;.JS;=;%
    node  "%~dp0\..\nuxt\bin\nuxt.js" %~dp1%~1\%~2
  )
) ELSE IF %1 EQU "vue3" (
  SET VUE_CLI_CONTEXT=%~dp1%~1\%~2
  @IF EXIST "%~dp0\node.exe" (
    "%~dp0\node.exe"  "%~dp0\..\@vue\cli-service\bin\vue-cli-service.js" serve
  ) ELSE (
    @SETLOCAL
    @SET PATHEXT=%PATHEXT:;.JS;=;%
    node  "%~dp0\..\@vue\cli-service\bin\vue-cli-service.js" serve
  )
)